{
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "none",
        "ignoreRestSiblings": false
        }
    ],
    "func-names": "off",
    "consistent-return": "off"
  }
}